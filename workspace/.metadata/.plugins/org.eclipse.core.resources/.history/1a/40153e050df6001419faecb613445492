package forumPackage;

public class StoreInfo {
	private String Name,Roll,Address,Gender,Hobby;
	public StoreInfo(String Name,String Roll,String Address,String Gender,String Hobby){
		this.Name = Name;
		this.Address = Address;
		this.Roll = Roll;
		this.Gender = Gender;
		this.Hobby = Hobby;
	}
	public String toString(){
		String output =  String.format("%s %s %s %s %s %s\n",Name,Roll,Address,Gender,Hobby);
		return output;
	}
	public void print(){
		System.out.println(this);
	}
}
