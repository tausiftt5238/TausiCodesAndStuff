package bounceTest;

import javax.swing.JPanel;
import java.awt.Graphics;
import javax.swing.JFrame;

class ball extends JPanel
{
	private int x;
	private int y;
	ball(int a,int b)
	{
		x = a;
		y = b;
	}
	public void paintComponent(Graphics g)
	{
		g.drawOval(x, y, 10, 10);
	}
	public void change(int a, int b)
	{
		x = a;
		y = b;
	}
}

public class Bounce 
{

}
